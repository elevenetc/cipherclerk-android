<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:parentTag="com.elevenetc.cipherclerk.android.lock.LockKeyboard">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_1"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="1" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_2"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_1"
        app:layout_constraintTop_toTopOf="parent"
        android:text="2" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_3"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_2"
        app:layout_constraintTop_toTopOf="parent"
        android:text="3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_4"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_1"
        android:text="4" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_5"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_1"
        app:layout_constraintTop_toBottomOf="@id/btn_2"
        android:text="5" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_6"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_2"
        app:layout_constraintTop_toBottomOf="@id/btn_3"
        android:text="6" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_7"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_4"
        android:text="7" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_8"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_7"
        app:layout_constraintTop_toBottomOf="@id/btn_5"
        android:text="8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_9"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_8"
        app:layout_constraintTop_toBottomOf="@id/btn_6"
        android:text="9" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_0"
        android:gravity="center"
        style="@style/LockButton"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toRightOf="@id/btn_7"
        app:layout_constraintTop_toBottomOf="@id/btn_8"
        android:text="0" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_del"
        style="@style/LockButton"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/btn_0"
        app:layout_constraintTop_toBottomOf="@id/btn_9"
        android:text="del" />


</merge>